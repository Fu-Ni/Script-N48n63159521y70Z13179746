local repo = "https://raw.githubusercontent.com/deividcomsono/Obsidian/main/"
local Library = loadstring(game:HttpGet(repo.."Library.lua"))()

local supported = {
    [13704594433] = true,
    [73956553001240] = true,
}

local invite = "https://discord.gg/3NjyJjjXxF"

if not supported[game.PlaceId] then
    setclipboard(invite)
    Library:Notify("❌ Game not supported — Discord link copied. Join for updates & keyless scripts.", 5)
    return
end

setclipboard(invite)
Library:Notify("✔ Game supported — Discord link copied. Join for keyless scripts.", 5)
